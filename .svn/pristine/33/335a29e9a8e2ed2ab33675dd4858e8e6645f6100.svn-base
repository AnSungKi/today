@charset "utf-8";

/** Animation **/
.box,
.go_btn,
.sns .link_btn
{transition : all 0.4s ease; -webkit-transition : all 0.4s ease;}


.box {width:33.333%; float:left; height:100%;}
.box50 {width:50%; float:left; height:100%;}
.mar {background:#fff; height:100%; box-sizing:border-box; padding:84px 6% 15px 6%; border-radius:5px; -webkit-border-radius:5px; box-shadow:0 0 8px #cdd5db; -webkit-box-shadow:0 0 8px #cdd5db; overflow:hidden; position:relative;}
.go_btn {position:absolute; right:6%; top:22px; background:#eff3f6; display:inline-block; vertical-align:top; padding:3px 15px 5px 15px; text-align:center; border-radius:16px; -webkit-border-radius:16px;}
.go_btn:hover, .go_btn:active {background:#dadfe2;}
.go_btn span {display:inline-block; vertical-align:top; padding:0 15px 0 0; color:#777879; background:url(../images/contents/go_btn.png) no-repeat right 50%;}
.mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar {width:2px; opacity:0;}
.mCSB_scrollTools .mCSB_draggerRail {background-color: rgba(0,0,0,0);}
.cont {height:100%; width:100%; position:relative; display:table;}
.cont .elem {display:table-cell; vertical-align:middle;}
.cont .elem:after {content:""; display:block; clear:both;}
.cont01 {height:96%; width:100%; position:relative; display:table;}
.cont01 .vote {display:table-cell; padding-top:50px;}
.cont01 .vote:after {content:""; display:block; clear:both;}

.sns {height:100%; box-sizing:border-box; padding:3px 8px;}
.sns .mar {padding-bottom:70px; padding-top:85px;}
.sns .go_btn span {padding:0 0 0 28px; color:#777879; background:url(../images/contents/refresh_ico.png) no-repeat left 50%;}
.sns .list {height:100%; box-sizing:border-box; position:relative;}
.sns .list:before {content:""; display:block; width:3px; height:100%; background:#eaeeef; position:absolute; left:50px; top:15px;}
.sns .list ul {position:relative;}
.sns .list ul li {border:1px solid #e0e0e2; background:#fff; margin:30px 9px 0 9px; border-radius:5px; -webkit-border-radius:5px; padding:17px 32px; position:relative;}
.sns .list ul li:first-child {margin-top:15px;}
.sns .list ul li:nth-child(odd) {background:#f8fcff;}
.sns .list ul li p {margin:0; color:#333; line-height:19px;}
.sns .list ul li p.date {margin-top:10px; color:#909196; padding:0 0 0 20px; font-size:13px; background:url(../images/contents/date_ico.png) no-repeat left 3px;}
.sns .list ul li:before {content:""; display:block; width:40px; height:40px; border-radius:50%; -webkit-border-radius:50%; position:absolute; left:-10px; top:-15px; overflow:hidden;}
.sns .list ul li:after {content:""; display:block; width:24px; height:24px; position:absolute; right:15px; bottom:15px; overflow:hidden;}
.sns .list ul li.ico1:before {background:#3a589e url(../images/contents/sns_ico1.png) no-repeat center 50%;}
.sns .list ul li.ico2:before {background:#7c4dff url(../images/contents/sns_ico2.png) no-repeat center 50%;}
.sns .list ul li.ico3:before {background:#fce102 url(../images/contents/sns_ico3.png) no-repeat center 50%;}
.sns .list ul li.ico4:before {background:#59b5e8 url(../images/contents/sns_ico4.png) no-repeat center 50%;}
.sns .list ul li.ico5:before {background:url(../images/contents/sns_ico5.png) no-repeat center 50%;}
.sns .list ul li.ico6:before {background:url(../images/contents/sns_ico6.png) no-repeat center 50%;}
.sns .list ul li.ico7:before {background:#77b9f1 url(../images/contents/sns_ico7.png) no-repeat center 50%;}
/* .sns .list ul li.ico8:before {background:#f27056 url(../images/contents/sns_ico8.png) no-repeat center 50%;} */
.sns .link_btn {display:block; width:88%; text-align:center; background:#90aee9; color:#fff; padding:12px 0; font-size:16px; border-radius:25px; -webkit-border-radius:25px; position:absolute; left:6%; bottom:20px; height:26px; text-decoration:none;}
.sns .link_btn:hover, .sns .link_btn:active {background:#7697d6;}


