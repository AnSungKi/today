.weather {height:31%; box-sizing:border-box; padding:3px 8px;}
.weather .detail {width:55%; float:left; position:relative; box-sizing:border-box; padding:10px 5% 0 120px; height:105px;}
.weather .detail:after {content:""; display:block; width:1px; height:70%; background:#e2e2e2; position:absolute; right:0; top:15%;}
.weather .detail .icon {display:block; width:106px; position:absolute; left:0; top:0;}
.weather .detail p {margin:0;}
.weather .detail p.temp {font-size:35px; line-height:35px; letter-spacing:-1px; color:#404a67; height:35px; overflow:hidden;}
.weather .detail p.temp em {font-size:21px; line-height:21px; display:inline-block; vertical-align:top; margin-top:3px;}
.weather .detail p.add {margin:10px 0 0 0;}
.weather .detail p.add strong {display:block; font-size:15px; line-height:19px; color:#444; font-weight:normal;}
.weather .detail p.add span {display:block; color:#909198; line-height:19px;}
.weather .etc_infor {width:45%; float:left; box-sizing:border-box; padding:15px 0 0 7%;}
.weather .etc_infor li {padding:0 0 0 38px; font-size:15px; color:#666; min-height:30px; line-height:30px; margin-top:13px;}
.weather .etc_infor li:first-child {margin-top:0;}
.weather .etc_infor li.ico1 {background:url(../images/contents/weather_infor_ico1.png) no-repeat left top;}
.weather .etc_infor li.ico2 {background:url(../images/contents/weather_infor_ico2.png) no-repeat left top;}
	
.schedule {height:69%; box-sizing:border-box; padding:3px 8px;}
.schedule .mar {padding-bottom:20px; padding-top:95px;}
.schedule .list {height:100%; box-sizing:border-box;}
.schedule .list ul {position:relative;}
.schedule .list ul li {margin:22px 0 0 0; padding:3px 0 3px 100px; position:relative; min-height:67px;}
.schedule .list ul li:first-child {margin-top:0;}
.schedule .list ul li p {margin:0; color:#333; line-height:19px;}
.schedule .list ul li p.cal {position:absolute; left:0; top:0; width:85px; height:100%; padding:10px 10px 0 10px; box-sizing:border-box; background:#f6f6f6; border-radius:5px; -webkit-border-radius:5px; text-align:center;}
.schedule .list ul li:nth-child(even) p.cal {background:#eef5fa;}
.schedule .list ul li p.cal span {display:block;}
/* .schedule .list ul li p.cal span.time {font-size:26px; line-height:22px;} */
.schedule .list ul li p.cal span.time {font-size:26px; line-height:48px;}
.schedule .list ul li p.posi {display:inline-block; background:#77b8f0 url(../images/contents/schedule_posi_ico.png) no-repeat 10px 50%; color:#fff; text-align:center; padding:2px 10px 3px 20px; font-size:13px; border-radius:13px; -webkit-border-radius:13px;}
.schedule .list ul li p.txt {font-size:15px; margin:5px 0 0 0;}
.schedule .list ul li p.depart {color:#909196; font-size:13px;}

.population {height:31%; box-sizing:border-box; padding:3px 8px;}
.population .state {background:url(../images/contents/population_bg.png) no-repeat left top; height:105px; padding:3px 0 0 135px; margin:0 7%; box-sizing:border-box;}
.population .state p {margin:0;}
.population .state p.member {color:#404a67;}
.population .state p.member span {display:block; color:#585858; font-size:16px; line-height:22px; margin-bottom:2px;}
.population .state p.member strong {display:block; font-size:43px; line-height:43px; font-weight:normal;}
.population .state p.member strong em {font-size:23px;}
.population .state p.date {color:#909198;}

.finance {height:38%; box-sizing:border-box; padding:3px 8px;}
.finance ul {width:100%; padding:18px 0 0 0;}
.finance ul:after {content:""; display:block;  clear:both;}
.finance ul li {float:left; width:49.5%; margin-left:1%; background:url(../images/contents/finance_elem_bg2.jpg) repeat; border-radius:5px; -webkit-border-radius:5px; text-align:center; padding:35px 0 13px 0; position:relative;}
.finance ul li:after {content:""; display:block; width:60px; height:60px; background:url(../images/contents/finance_elem_ico2.png) no-repeat; position:absolute; left:50%; top:-30px; margin:0 0 0 -30px;}
.finance ul li:first-child:after {background:url(../images/contents/finance_elem_ico1.png) no-repeat;}
.finance ul li:first-child {margin-left:0; background:url(../images/contents/finance_elem_bg1.jpg) repeat;}
.finance ul li p {margin:0;}
.finance ul li p.money span {display:block; font-size:16px; line-height:22px; color:#444;}
.finance ul li p.money strong {display:block; font-size:35px; line-height:30px; font-weight:normal; color:#404a67;}
.finance ul li p.money strong em {font-size:19px;}
.finance ul li p.percent {color:#909198;}

.job {height:31%; box-sizing:border-box; padding:3px 8px;}
.job ul {width:100%;}
.job ul:after {content:""; display:block;  clear:both;}
.job ul li {float:left; width:47.5%; margin-left:5%; position:relative; text-align:center;}
.job ul li:first-child {margin-left:0;}
.job ul li a {display:inline-block; color:#444; font-size:16px; padding:0 22px 0 0; background:url(../images/contents/blank_ico.png) no-repeat right 50%;}
.job ul li ul {width:auto; padding:0 115px 0 0; margin:10px 0 0 0;}
.job ul li ul li {float:none; width:auto; margin:0; text-align:left; font-size:15px; line-height:19px; padding:4px 0 4px 4px; color:#909198; background:url(../images/contents/job_bul.png) no-repeat left 14px;}
.job ul li ul li strong {font-size:25px; font-weight:normal;}
.job ul li ul li em{font-size:20px; font-weight:normal;}
.job ul li ul li:first-child {color:#005bab; border-bottom:1px solid #7fadd5; background:url(../images/contents/job_bul_on.png) no-repeat left 14px;}
.job ul li p {margin:0; width:95px; position:absolute; right:0; top:38px; text-align:center; padding:8px 0; border-radius:60px; -webkit-border-radius:60px;}
.job ul li p span {font-size:20px; line-height:27px; color:#404a67; display:inline-block; padding:0 0 0 21px;}
.job ul li p span em {font-size:19px;}
.job ul li p.up {border:2px solid #b1e2bc;}
.job ul li p.up span {background:url(../images/contents/job_arrow_up.png) no-repeat left 2px;}
.job ul li p.down {border:2px solid #fad6d4;}
.job ul li p.down span {background:url(../images/contents/job_arrow_down.png) no-repeat left 3px;}


